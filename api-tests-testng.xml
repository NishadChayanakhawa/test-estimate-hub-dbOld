<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Test Estimate Hub API Tests" parallel="tests" thread-count="5">
	<listeners>
		<listener
			class-name="io.nishadc.automationtestingframework.testngcustomization.ReportGenerator"></listener>
	</listeners>
	<test name="Coverage Tests" parallel="methods" thread-count="1">
		<classes>
			<class
				name="io.github.nishadchayanakhawa.testestimatehub.tests.apitests.CoverageTests" />
		</classes>
	</test>
	<test name="Controller Tests" parallel="methods" thread-count="1">
		<classes>
			<class
				name="io.github.nishadchayanakhawa.testestimatehub.tests.apitests.ControllerTests" />
		</classes>
	</test>
	<test name="API Tests : User Management" parallel="methods" thread-count="1">
		<classes>
			<class
				name="io.github.nishadchayanakhawa.testestimatehub.tests.apitests.UserManagementApiTests" />
		</classes>
	</test>
	<test name="API Tests : Test Type Configuration" parallel="methods" thread-count="1">
		<classes>
			<class
				name="io.github.nishadchayanakhawa.testestimatehub.tests.apitests.TestTypeConfigurationApiTests" />
		</classes>
	</test>
	<test name="API Tests : Change Type Configuration" parallel="methods" thread-count="1">
		<classes>
			<class
				name="io.github.nishadchayanakhawa.testestimatehub.tests.apitests.ChangeTypeConfigurationApiTests" />
		</classes>
	</test>
	<test name="API Tests : General Configuration" parallel="methods" thread-count="1">
		<classes>
			<class
				name="io.github.nishadchayanakhawa.testestimatehub.tests.apitests.GeneralConfigurationApiTests" />
		</classes>
	</test>
	<test name="API Tests : Application Configuration" parallel="methods" thread-count="1">
		<classes>
			<class
				name="io.github.nishadchayanakhawa.testestimatehub.tests.apitests.ApplicationConfigurationApiTests" />
		</classes>
	</test>
	<test name="API Tests : Release Management" parallel="methods" thread-count="1">
		<classes>
			<class
				name="io.github.nishadchayanakhawa.testestimatehub.tests.apitests.ReleaseManagementApiTests" />
		</classes>
	</test>
	<test name="API Tests : Change Management" parallel="methods" thread-count="1">
		<classes>
			<class
				name="io.github.nishadchayanakhawa.testestimatehub.tests.apitests.ChangeManagementApiTests" />
		</classes>
	</test>
</suite>